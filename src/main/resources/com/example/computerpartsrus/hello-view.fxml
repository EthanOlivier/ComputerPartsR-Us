<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vbxMain" alignment="CENTER" prefHeight="695.0" prefWidth="592.0" spacing="20.0" style="-fx-background-color: #ADD8E6;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.computerpartsrus.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane fx:id="anchorPane" prefHeight="655.0" prefWidth="898.0">
      <children>
          <Button fx:id="btnInformation" disable="true" layoutX="75.0" layoutY="151.0" onAction="#onbtnInformationClick" prefHeight="33.0" prefWidth="149.0" text="Enter" />

          <Label prefHeight="25.0" prefWidth="94.0" text="Customer Name:" />
         <TextField fx:id="txtName" layoutY="25.0" onKeyTyped="#ontxtNameClick" promptText="Joe Smoe" />
         <Label layoutX="158.0" layoutY="6.0" text="Address:" />
         <TextField fx:id="txtAddress" layoutX="161.0" layoutY="25.0" onKeyTyped="#ontxtAddressClick" promptText="123 Front Street" />
         <Label layoutX="4.0" layoutY="51.0" text="City" />
         <TextField fx:id="txtCity" layoutX="1.0" layoutY="67.0" onKeyTyped="#ontxtCityClick" promptText="Any Town" />
         <Label layoutX="158.0" layoutY="50.0" text="State" />
         <TextField fx:id="txtState" layoutX="161.0" layoutY="67.0" onKeyTyped="#ontxtStateClick" promptText="PA" />
         <Label layoutX="1.0" layoutY="92.0" text="Zip" />
         <TextField fx:id="txtZip" layoutX="1.0" layoutY="109.0" onKeyTyped="#ontxtZipClick" promptText="54321" />
         <ComboBox fx:id="cbxItem" disable="true" layoutX="60.0" layoutY="231.0" onAction="#oncbxItemEnter" prefHeight="25.0" prefWidth="193.0" />
         <TextField fx:id="txtQuantity" disable="true" layoutX="1.0" layoutY="231.0" onKeyTyped="#ontxtQuantityClick" prefHeight="25.0" prefWidth="46.0" />
         <ComboBox fx:id="cbxType" disable="true" layoutX="274.0" layoutY="231.0" onAction="#oncbxTypeEnter" prefHeight="25.0" prefWidth="193.0" />
         <Button fx:id="btnItems" disable="true" layoutX="488.0" layoutY="231.0" mnemonicParsing="false" onAction="#onbtnItemsClick" prefHeight="25.0" prefWidth="136.0" text="Enter" />
         <Label layoutY="206.0" text="Quantity" />
         <Label layoutX="63.0" layoutY="206.0" text="Item" />
         <Label layoutX="274.0" layoutY="206.0" text="Type" />
         <Label fx:id="lblOutput" alignment="TOP_LEFT" layoutY="260.0" prefHeight="322.0" prefWidth="786.0" />
         <Button fx:id="btnFinish" disable="true" layoutX="648.0" layoutY="231.0" mnemonicParsing="false" onAction="#onbtnFinishClick" prefHeight="25.0" prefWidth="136.0" text="Finish" />
         <Label fx:id="lblDaysOutput" alignment="TOP_LEFT" layoutX="3.0" layoutY="582.0" prefHeight="33.0" prefWidth="403.0" text="Cumulative Day's Quantity:        Cumulative Day's Profit: " />
         <Button fx:id="btnPrint" disable="true" layoutX="4.0" layoutY="620.0" mnemonicParsing="false" onAction="#onbtnPrintClick" prefHeight="25.0" prefWidth="84.0" text="Print" />
         <Button fx:id="btnClear" disable="true" layoutX="364.0" layoutY="620.0" mnemonicParsing="false" onAction="#onbtnClearClick" prefHeight="25.0" prefWidth="84.0" text="Clear Order" />
         <Button fx:id="btnClose" layoutX="695.0" layoutY="620.0" mnemonicParsing="false" onAction="#onbtnCloseClick" prefHeight="25.0" prefWidth="84.0" text="Close" />
         <CheckBox fx:id="chbxMember" layoutX="163.0" layoutY="113.0" mnemonicParsing="false" text="Member?" />
         <TextField fx:id="txtDate" editable="false" layoutX="652.0" layoutY="2.0" prefHeight="25.0" prefWidth="127.0" />
      </children>
   </AnchorPane>
</VBox>
